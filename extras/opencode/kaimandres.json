{
  "$schema": "https://opencode.ai/theme.json",
  "defs": {
    "deepBlack": "#0e0e15",
    "darkestGray": "#111116",
    "darkGray": "#16161e",
    "mediumDarkGray": "#1a1a22",
    "borderGray": "#24242e",
    "activeGray": "#2a2a37",
    "highlightGray": "#353749",
    "muted": "#767c9d",
    "mutedLight": "#7a7f98",
    "foreground": "#e4e4eb",
    "blue": "#78a9ff",
    "blueLight": "#89ddff",
    "blueMedium": "#8ab8ff",
    "blueBright": "#add7ff",
    "blueHover": "#6394e6",
