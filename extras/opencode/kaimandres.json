    "syntaxVariable": {
      "dark": "foreground",
      "light": "darkGray"
    },
    "syntaxString": {
      "dark": "greenBright",
      "light": "green"
    },
    "syntaxNumber": {
      "dark": "orange",
      "light": "orange"
    },
    "syntaxType": {
      "dark": "yellowMedium",
      "light": "orange"
    },
    "syntaxOperator": {
      "dark": "teal",
      "light": "teal"
